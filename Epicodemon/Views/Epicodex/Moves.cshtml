<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <link href="~/css/bootstrap.css" rel="stylesheet" type="text/css">
  <link href="~/css/styles.css" rel="stylesheet" type="text/css">
  <script src="~/js/jquery-3.3.1.js"></script>
  <script src="~/js/scripts.js"></script>
  <title>Epicodemon</title>
</head>
<body>
  <div class="container">
    <h1>Epicodex</h1>
    @foreach(var move in Model)
    {
      <div class="well">
        <h4>@move.GetName()</h4>
        <p>Description: @move.GetDescription()</p>
        <p>Attack Style: @move.GetAttackStyle()</p>
        <p>Base Power: @move.GetBasePower()</p>
        <p>Accuracy: move.GetAccuracy()</p>
        <p>Power Points: move.GetPowerPoints()</p>
        <p>Secondary Effect: @move.GetSecondaryEffect()</p>
      </div>
    }
</body>
</html>
